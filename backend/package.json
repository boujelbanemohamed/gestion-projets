"scripts": {
  "start": "node -r ts-node/register/transpile-only src/server.ts",
  "build": "tsc",
  "dev": "node server-final.js",
  "dev-ts": "nodemon src/server.ts",
  "test": "node run_tests.js",
  "test:postman": "newman run postman_collection.json -e environment.json",
  "test:all": "npm run test",
  "test:report": "npm run test && open test_report.html",
  "install:newman": "npm install -g newman",
  "setup": "npm install && npm run install:newman"
}